<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="generator" content="{{app_version}}">
	<title>{{title}}</title>

[[assets_head]]

</head>
<body id="{{view_name}}">
	<div id="page">
		<div id="header">
			<ul class="nav">
			{{#main_navigation}}
				<li><a href="{{url}}">{{text}}</a></li>
			{{/main_navigation}}
			</ul>
			<h1>{{title}}</h1>
			{{#notices}}
				<div class="notice {{type}}">{{message}}</div>
			{{/notices}}
		</div>
		<div id="content">
		{{>body}}
		</div>
		<div id="footer"></div>
	</div>
	[[assets_body]]
</body>
</html>
